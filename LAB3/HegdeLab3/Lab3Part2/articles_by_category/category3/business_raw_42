            window.NYTD.Abra('www-story-1427-ad-aggro') === 'pro-4');

        if ((adAggro || adAggro2) &&
            document.documentElement.className.indexOf('article-has-layout-large-horizontal') === -1
        ) {
                // only run this test if it's large or jumbo horizontal lede media
