(function () {
    var ribbon;
    if (window.magnum.getFlags().indexOf('adAggro') !== -1 &&
        (window.NYTD.Abra('www-story-pro-agro') !== 'control' &&
        window.NYTD.Abra('www-story-pro-agro') !== null &&
