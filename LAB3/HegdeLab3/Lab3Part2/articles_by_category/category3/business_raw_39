<script>
require(['foundation/main'], function() {
  require(['jquery/nyt'], function($) {
    if ( $('.g-custom-span strong').length ) {
      $('.g-custom-span strong').html($('.g-custom-span strong').html().replace(':',''));
    }

    // hide additional kicker on desktop
    var kickerHTML = $('#story-meta .kicker-label');
    $('#story-meta .kicker').html(kickerHTML);

    // updated time
    var time = document.querySelector('meta[property="article:modified"]').getAttribute('content');
    var now = new Date().getTime();
    var date = new Date(time);
    var anHour = 60 * 60 * 1000;
    var aDay = 24 * anHour;

    // mobile iframe
    if ( window.self != window.top && ( window.location.host.indexOf('mobile') > -1 || window.location.href.indexOf('embedded.html') > -1 ) ) {

      // ignoring mobile these days
      // document.querySelector('html').classList += ' live-briefing-template-embed';

      // var article = window.parent.document;
      // article.querySelector('html').classList += ' live-briefing-mobile-theme';
      // var mobileStyle = document.querySelector('#g-mobile-style').outerHTML;
      // article.querySelector('head').insertAdjacentHTML('beforeend', mobileStyle);

      // if ( now - date.getTime() < anHour*8 ) {
      //   var originalHeadline = article.querySelector('.full-art .headline').innerHTML;
      //   article.querySelector('.full-art .headline').innerHTML = "<span class='g-headline-kicker'>" + briefingKickerText + "</span><br><div class='g-headline-proper'>" + originalHeadline + "</div>";
      // }

    } else {

      var body = document.querySelector('body');
      var themeClass = 'live-briefing-theme';
      var briefingKickerText = $('meta#column-name').length ? $('meta#column-name').attr('content') : 'Live Briefing';

      // don't run this script more than once
      if ( !body.classList.contains(themeClass) ) {

        document.querySelector('body').classList += ' ' + themeClass;

        // if updated less than a day ago, show update time
        if ( now - date.getTime() < aDay ) {
          var hours = date.getHours() > 11 ? date.getHours()-12 : date.getHours();
          if ( hours == 0 ) hours = 12;
          var minutes = date.getMinutes() < 10 ? '0' + date.getMinutes() : date.getMinutes();
          var ampm = date.getHours() > 11 ? 'PM' : 'AM';
          var formattedDate = hours + ':' + minutes + ' ' + ampm;
          document.querySelector('.dateline').innerHTML = 'UPDATED <span class="g-updated">' + formattedDate + '</span>';
          document.querySelector('.dateline').classList += ' g-updated';
        }

        require(['foundation/main'], function() {
          require(['jquery/nyt'], function($) {

            if ( now - date.getTime() < anHour*8 ) {
              $('.story-header .headline')
                .html("<span class='g-headline-kicker'>" + briefingKickerText + "</span><br><div class='g-headline-proper'>" + $('.story-header .headline').html() + "</div>");
            }

            var alert = $('.g-custom-span');
            if ( alert.length ) {
              alert.closest('.story-body-supplemental').addClass('g-alert-container');
            }

          });
        });
      }
      
    //   // ad placements — Tom G.
    //   ad placements remove because of ad test - Meghan L.
    //  require(['article/story/main'], function() {
	   //   require(['article/story/instances/ad-manager'], function(dfp) {

    //       if (window.innerWidth < 1024) return;

    //       window.$ = $;

    //       // ads before live briefing subheads
    //       var threshold = 2;
    //       var stride = 3;
    //       var maximum = 3;
    //       var selector = '.story-subheading';
    //       var adConfig = {
    //         sizes: {
    //           mobile: [[414,457], [300,250], [320,50]],
    //           tablet: ['fluid', [1605,300], [728,90]],
    //           desktop: ['fluid', [1605,300], [970,250], [970,90], [728,90]]
    //         }
    //       };

    //       var index = 0;
