<script>(function(){
  document.addEventListener('DOMContentLoaded', function(e) {
    // Add multiple roles in byline
    var bylines = document.documentElement.querySelectorAll('#story-header .byline'),
        credit = document.documentElement.querySelector('#story-header .credit'),
        roles = { text : [], photo : [] };
    for(var i = 0; i < bylines.length; i++) {
      var b = bylines[i];
      var a = b.querySelector('.byline-author');
      if(credit && credit.textContent.toLowerCase().indexOf(a.textContent.toLowerCase()) > -1){
        roles.photo.push(b);
      }else{
        roles.text.push(b);
      }
    }
    var prependText = function(el, str){
      if(el.firstChild.nodeType != 3) el.insertBefore(document.createTextNode(str), el.firstChild);
      else el.firstChild.nodeValue = str;
    };
    if(roles.photo.length){
      prependText(roles.photo[0], 'Photographs by ');
      roles.photo[0].lastChild.nodeValue = '';
      prependText(roles.text[0], 'Text by ');
    }
    // Identify headline length
    var headline = document.documentElement.querySelector('#story-header .headline');
    headline.className += ' ' + (headline.textContent.split(/\s+/).length > 7 ? 'long' : 'short');
    // Make diptychs with adjacent jumbo images
    var isJumbo = function(el) {
      if(!el) return false;
      if(/jumbo/.test(el.className)) return true;
      if(el.firstElementChild) return /jumbo/.test(el.firstElementChild.className);
    };
    var interrupters = document.documentElement.querySelectorAll('.story.theme-main > .story-interrupter, .story.theme-minimal .media');
    var n = 0;
    for(var i = 0; i < interrupters.length; i++){
      var el = interrupters[i];
      if(isJumbo(el) && (isJumbo(el.nextElementSibling) || isJumbo(el.previousElementSibling))) {
        el.className += ' diptych';
        el.className += ' ' + (n%2 ? 'nth-2' : 'nth-1');
        n++;
      } else {
        n = 0;
      }
    }
  });
})();

// https://static01.nyt.com/services/json/sectionfronts/magazine/index.jsonp?callback=jsonFeedCallback_magazine
;
define("script", function(){});

</script>
